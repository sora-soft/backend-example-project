{
  "name": "example-project",
  "version": "1.0.0",
  "description": "sora system example",
  "bin": {
    "app-server": "./bin/cli.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && ttsc",
    "dev": "npm run build && node ./bin/cli.js run server --config ./config.yml -w ./run",
    "migrate:sync": "node ./bin/cli.js command --config ./config.yml -w ./run -n database-migrate sync",
    "migrate": "node ./bin/cli.js command --config ./config.yml -w ./run -n database-migrate migrate",
    "migrate:drop": "node ./bin/cli.js command --config ./config.yml -w ./run -n database-migrate drop",
    "migrate:revert": "node ./bin/cli.js command --config ./config.yml -w ./run -n database-migrate revert",
    "migrate:generate": "node ./bin/cli.js command --config ./config.yml -w ./run -n database-migrate generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "link": "npm link @sora-soft/framework @sora-soft/etcd-discovery @sora-soft/http-support @sora-soft/redis-component @sora-soft/database-component @sora-soft/etcd-component"
  },
  "repository": "git@github.com/sora-soft/example-project.git",
  "keywords": [
    "sora"
  ],
  "author": "yaya",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/sora-soft/example-project/issues"
  },
  "homepage": "https://github.com/sora-soft/example-project#readme",
  "dependencies": {
    "@sora-soft/database-component": "^1.0.4",
    "@sora-soft/etcd-component": "^1.0.3",
    "@sora-soft/etcd-discovery": "^1.0.8",
    "@sora-soft/framework": "^1.0.13",
    "@sora-soft/http-support": "^1.0.6",
    "@sora-soft/redis-component": "^1.0.6",
    "axios": "^0.21.1",
    "camelcase": "^6.2.0",
    "class-validator": "^0.13.1",
    "commander": "^7.2.0",
    "js-yaml": "^4.0.0",
    "koa": "^2.13.1",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "typeorm": "^0.2.32",
    "typescript-is": "^0.18.2"
  },
  "devDependencies": {
    "@types/assert": "^1.5.4",
    "@types/axios": "^0.14.0",
    "@types/expect": "^24.3.0",
    "@types/js-yaml": "^4.0.0",
    "@types/koa": "^2.13.1",
    "@types/mocha": "^8.2.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.37",
    "@types/source-map-support": "^0.5.3",
    "@types/uuid": "^8.3.0",
    "assert": "^2.0.0",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3"
  }
}
